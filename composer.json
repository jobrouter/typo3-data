{
	"name": "brotkrueml/typo3-jobrouter-data",
	"description": "Connect JobRouter® JobData tables with TYPO3",
	"license": "GPL-2.0-or-later",
	"type": "typo3-cms-extension",
	"keywords": [
		"TYPO3 CMS",
		"TYPO3 extension",
		"JobRouter",
		"Data",
		"Workflow"
	],
	"authors": [
		{
			"name": "Chris Müller",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/brotkrueml/typo3-jobrouter-data/issues",
		"source": "https://github.com/brotkrueml/typo3-jobrouter-data",
		"docs": "https://typo3-jobrouter.readthedocs.io/projects/data/"
	},
	"require": {
		"php": ">=7.3",
		"ext-json": "*",
		"ext-pdo": "*",
		"brotkrueml/jobrouter-client": "^1.0",
		"brotkrueml/typo3-jobrouter-base": "^1.2@dev",
		"brotkrueml/typo3-jobrouter-connector": "^1.0",
		"symfony/polyfill-intl-icu": "^1.15",
		"symfony/polyfill-php80": "^1.17",
		"typo3/cms-backend": "^10.4.11 || ^11.5.3",
		"typo3/cms-core": "^10.4.11 || ^11.5.3",
		"typo3/cms-extbase": "^10.4.11 || ^11.5.3",
		"typo3/cms-fluid": "^10.4.11 || ^11.5.3",
		"typo3/cms-frontend": "^10.4.11 || ^11.5.3"
	},
	"require-dev": {
		"brotkrueml/coding-standards": "~1.2.0",
		"codeception/codeception": "^4.1",
		"codeception/module-asserts": "^1.3",
		"codeception/module-webdriver": "^1.4",
		"dg/bypass-finals": "^1.3",
		"donatj/mock-webserver": "^2.2",
		"ergebnis/composer-normalize": "~2.20.0",
		"mikey179/vfsstream": "^1.6",
		"php-coveralls/php-coveralls": "^2.4",
		"phpstan/extension-installer": "^1.1",
		"phpstan/phpstan": "1.4.2",
		"phpunit/phpunit": "^9.5",
		"rector/rector": "0.12.13",
		"saschaegerer/phpstan-typo3": "^1.0",
		"symfony/yaml": "^4.4 || ^5.4 || ^6.0",
		"symplify/phpstan-rules": "^10.0",
		"typo3/cms-dashboard": "^10.4.11 || ^11.5.3",
		"typo3/cms-fluid-styled-content": "^10.4.11 || ^11.5.3",
		"typo3/cms-form": "^10.4.11 || ^11.5.3",
		"typo3/testing-framework": "^6.12"
	},
	"suggest": {
		"ext-intl": "Format dates and numbers in JobData content element columns",
		"typo3/cms-dashboard": "Use dashboard widgets for information about JobData transmissions",
		"typo3/cms-fluid-styled-content": "Necessary when using the table content element",
		"typo3/cms-form": "Use form finisher with TYPO3 form framework"
	},
	"autoload": {
		"psr-4": {
			"Brotkrueml\\JobRouterData\\": "Classes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Brotkrueml\\JobRouterData\\Tests\\": "Tests"
		}
	},
	"config": {
		"allow-plugins": {
			"ergebnis/composer-normalize": true,
			"phpstan/extension-installer": true,
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true
		},
		"bin-dir": ".Build/bin",
		"sort-packages": true,
		"vendor-dir": ".Build/vendor"
	},
	"extra": {
		"branch-alias": {
			"dev-main": "1.0.x-dev"
		},
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"extension-key": "jobrouter_data",
			"web-dir": ".Build/Web"
		}
	},
	"scripts": {
		"post-autoload-dump": [
			"TYPO3\\TestingFramework\\Composer\\ExtensionTestEnvironment::prepare"
		]
	}
}
